{
    "main": "index.html",
    "name": "FLUX Studio",
    "node-remote": ["http://*/*", "*://*/*"],
    "description": "FLUX Studio",
    "version": "{Version}",
    "inject_js_start": "test-pipe.js",
    "window": {
        "title": "FLUX Studio",
        "icon": "icon.png",
        "frame": true,
        "width": 1280,
        "height": 640,
        "min_width": 1000,
        "min_height": 600
    },
    "nodejs": true,
    "chromium-args": "--mixed-context --enable-webgl --ignore-gpu-blacklist",
    "webkit": {
        "plugin": true
    }
}
